### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#e6cdc9'
set completion-bg '#090a0a'

# Command line completion group elements
set completion-group-fg '#B72B21'
set completion-group-bg '#090a0a'

# Current command line completion element
set completion-highlight-fg '#090a0a'
set completion-highlight-bg '#e6cdc9'

# Default foreground/background color
set default-bg rgba(9,10,10,1)

# Input bar
set inputbar-fg '#e6cdc9'
set inputbar-bg '#090a0a'

# Notification
set notification-fg '#e6cdc9'
set notification-bg '#090a0a'

# Error notification
set notification-error-fg '#e6cdc9'
set notification-error-bg '#B2291C'

# Warning notification
set notification-warning-fg '#e6cdc9'
set notification-warning-bg '#B2291C'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#e6cdc9'
set statusbar-bg '#090a0a'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#B72B21'
set highlight-active-color '#B72B21'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#e6cdc9'

# 'Loading...' text
set render-loading-fg '#e6cdc9'
set render-loading-bg '#090a0a'

# Index mode
set index-fg '#e6cdc9'
set index-bg '#090a0a'

# Selected element in index mode
set index-active-fg '#090a0a'
set index-active-bg '#e6cdc9'