### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#ebe8e9'
set completion-bg '#06040A'

# Command line completion group elements
set completion-group-fg '#30439C'
set completion-group-bg '#06040A'

# Current command line completion element
set completion-highlight-fg '#06040A'
set completion-highlight-bg '#ebe8e9'

# Default foreground/background color
set default-bg rgba(6,4,10,1)

# Input bar
set inputbar-fg '#ebe8e9'
set inputbar-bg '#06040A'

# Notification
set notification-fg '#ebe8e9'
set notification-bg '#06040A'

# Error notification
set notification-error-fg '#ebe8e9'
set notification-error-bg '#2C3694'

# Warning notification
set notification-warning-fg '#ebe8e9'
set notification-warning-bg '#2C3694'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#ebe8e9'
set statusbar-bg '#06040A'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#30439C'
set highlight-active-color '#30439C'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#ebe8e9'

# 'Loading...' text
set render-loading-fg '#ebe8e9'
set render-loading-bg '#06040A'

# Index mode
set index-fg '#ebe8e9'
set index-bg '#06040A'

# Selected element in index mode
set index-active-fg '#06040A'
set index-active-bg '#ebe8e9'