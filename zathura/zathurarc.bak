### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#dfe3e9'
set completion-bg '#0a102c'

# Command line completion group elements
set completion-group-fg '#8A6E8E'
set completion-group-bg '#0a102c'

# Current command line completion element
set completion-highlight-fg '#0a102c'
set completion-highlight-bg '#dfe3e9'

# Default foreground/background color
set default-bg rgba(10,16,44,1)

# Input bar
set inputbar-fg '#dfe3e9'
set inputbar-bg '#0a102c'

# Notification
set notification-fg '#dfe3e9'
set notification-bg '#0a102c'

# Error notification
set notification-error-fg '#dfe3e9'
set notification-error-bg '#4F7CC1'

# Warning notification
set notification-warning-fg '#dfe3e9'
set notification-warning-bg '#4F7CC1'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#dfe3e9'
set statusbar-bg '#0a102c'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#8A6E8E'
set highlight-active-color '#8A6E8E'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#dfe3e9'

# 'Loading...' text
set render-loading-fg '#dfe3e9'
set render-loading-bg '#0a102c'

# Index mode
set index-fg '#dfe3e9'
set index-bg '#0a102c'

# Selected element in index mode
set index-active-fg '#0a102c'
set index-active-bg '#dfe3e9'